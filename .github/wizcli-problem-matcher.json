{
    "problemMatcher": [
        {
            "owner": "wizcli-iac",
            "pattern": [
                {
                    "regexp": "Rule: (.+)$",
                    "message": 1
                },
                {
                    "regexp": "\\WFAILED.*$"
                },
                {
                    "regexp": "\\WFile: \\w+\/(.+):(\\d+)$",
                    "file": 1,
                    "line": 2
                }
            ],
            "severity": "warning"
        }
    ]
}
